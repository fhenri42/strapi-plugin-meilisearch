{
  "routes": [
    {
      "method": "GET",
      "path": "/reload",
      "handler": "meilisearch.reload",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/indexes",
      "handler": "meilisearch.deleteAllIndexes",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/indexes/:indexUid/update",
      "handler": "meilisearch.waitForDocumentsToBeIndexed",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/credentials",
      "handler": "meilisearch.getCredentials",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/credentials",
      "handler": "meilisearch.addCredentials",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/collections",
      "handler": "meilisearch.getCollections",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/collections/:collection",
      "handler": "meilisearch.addCollection",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/collections/:collection",
      "handler": "meilisearch.updateCollections",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/collections/:collection",
      "handler": "meilisearch.removeCollection",
      "config": {
        "policies": []
      }
    }
  ]
}
